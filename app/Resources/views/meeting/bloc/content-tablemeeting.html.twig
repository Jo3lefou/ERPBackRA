<div class="col-md-12 col-sm-12 col-xs-12">
  <div class="page-title">
    <div class="title_left">
      <h3>{% trans %}Meeting canceled{% endtrans %}</h3>
    </div>
  </div>
  <div class="x_panel">
    <div class="x_title">
      <h2>{% trans %}Find the meeting canceled{% endtrans %}</h2>
      <div class="clearfix"></div>
    </div>
    <div class="x_content">
      <table id="datatable-buttons" class="table table-striped table-bordered">
        <thead>
          <tr>
            <th></th>
            <th></th>
            <th></th>
            <th></th>
            <th></th>
            <th></th>
            <th></th>
            <th></th>
            <th></th>
            <th></th>
            <th></th>
            <th></th>
          </tr>
        </thead>
        <tbody>
			{% for meeting in meetings %}
				<tr id="meeting{{ meeting.id }}">
					<td>{{ meeting.customerName }}</td>
					<td>{{ meeting.title }}</td>
					<td>{{ meeting.comment }}</td>
					<td>{{ meeting.type }}</td>
					<td>{{ meeting.notifStatus }}</td>
					<td>{{ meeting.starta }}</td>
					<td>{{ meeting.enda }}</td>
					<td>{{ meeting.sale }}</td>
					<td>{{ meeting.namesale }}</td>
					<td>{{ meeting.color }}</td>
					<td>{{ meeting.lieu }}</td>
					<td>{{ meeting.lieuName }}</td>
				</tr>
			{% endfor %}
        </tbody>
      </table>
    </div>
  </div>
</div>