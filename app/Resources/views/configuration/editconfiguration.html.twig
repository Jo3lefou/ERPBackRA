                    var action = 'emailRdvCancelation';
                    break;
                case 'editor-efouren':
                    var action = 'emailRdvCancelationEn';
                    break;
                case 'editor-efive':
                    var action = 'emailRdvEdition';
                    break;
                case 'editor-efiveen':
                    var action = 'emailRdvEditionEn';
                    break;
                default:
                    var action = '';
            }
            var data = $('#'+reldata).html();
            //.replace(/&/g, '&amp').replace(/</g, '&lt;').replace(/>/g, '&gt;')
            //alert(data);
